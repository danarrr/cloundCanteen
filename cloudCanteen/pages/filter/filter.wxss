/* pages/canteenlist/canteenlist.wxss */
@import "/component/tipToast/tipToast.wxss";

.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #f6f9fb;
  color: #939393;
  padding: 0;
}

.selectbox {
  width: 706rpx;
  max-height: 85%;
  overflow: auto;
  display: block;
  background-color: #fff;
  padding: 0 36rpx;
  box-sizing: border-box;
  border: 1px solid #cbcbcb;
  border-radius: 10rpx;
  color: #626262;
  margin: 22rpx;
}

.selectbox text {
  display: inline-block;
  height: 96rpx;
  width: 100%;
  line-height: 96rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #cbcbcb;
}

.selectbox .last-text {
  border-bottom: 0px solid #cbcbcb;
}

.selectbox image {
  width: 52rpx;
  height: 52rpx;
  float: right;
  margin-top: -75rpx;
}

.select-confirm {
  width: 100%;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  background-color: #15c35a;
  color: #fff;
  position: absolute;
  bottom: 0;
  font-size: 30rpx;
}

.color-green {
  color: #19b34c;
}

.color-black {
  color: #626262;
}
