/* pages/room/room/index.wxss */

.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #f6f9fb;
  color: #939393;
  display: flex;
  /*flex-flow: row wrap;*//*justify-content: space-between;*/
  flex-direction: column;
  padding: 0;
  overflow: hidden;
}

.nav-top {
  flex: 0 0 auto;
  width: 100%;
  height: 70rpx;
  display: flex;
  padding: 21rpx 0;
  border-bottom: 1rpx solid #c9c9c9;
  background-color: #fff;
}

.nav-top .nav-top-item {
  width: 33.3%;
  height: 65rpx;
  /*line-height: 61rpx;*/
  text-align: center;
  color: #000;
  font-size: 28rpx;
  border-right: 1rpx solid #cacaca;
  /*white-space: nowrap;*/
  text-overflow: ellipsis;
  overflow: hidden;
}

.nav-top .triangle {
  transform: translate3d(-32rpx, 45rpx, 0);
  width: 0;
  height: 0;
  border-bottom: 17rpx solid #a3d5b7;
  border-left: 17rpx solid transparent;
}

.nav-top image {
  width: 34rpx;
  height: 34rpx;
  transform: translate3d(10rpx, 10rpx, 0);
}

.roomlist {
  width: 90%;
  border: 1px solid #ccc;
  margin: 0 auto;
  height: 140rpx;
  border-radius: 10rpx;
  margin-top: 22rpx;
  color: #000;
  font-size: 28rpx;
  padding: 0 30rpx;
  box-sizing: border-box
}

.leftlist {
  float: left;
  width: 70%;
 
}

.rightlist {
  float: right;
  /* transform: translateY(50%) */
  margin-top:50rpx;color:#585858
}

.listdata {
  height: 70rpx;
  line-height: 70rpx;
   color:#6e6e6e;
   font-size:26rpx
}
.listdata span{
  color:#585858;
}