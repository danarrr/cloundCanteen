.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #939393;
  display: flex;
  flex-direction: column;
  padding: 0;
  overflow: hidden;
  background: #f7f8fc;
}

input {
  display: block;
  height: 60rpx;
  text-overflow: clip;
  overflow: hidden;
  white-space: nowrap;
  font-family: UICTFontTextStyleBody;
  min-height: 1.4rem;
  border: 1px solid #ccc;
  width: 300rpx;
}

.contain {
  width: 100%;
  height: auto;
  background: #fff;
  border-top: 2rpx solid #ccc;
  margin-bottom: 20rpx;
  border-bottom: 2rpx solid #ccc;
  padding: 0rpx 63rpx;
}

.select-confirm {
  width: 100%;
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  background-color: #15c35a;
  color: #fff;
  position: absolute;
  bottom: 0;
  font-size: 30rpx;
}

.contain-li {
  height: 74rpx;
  line-height: 74rpx;
  font-size: 30rpx;
  border-bottom: 1rpx dashed #efefef;
}

.contain-li>view {
  color: #1cb34f;
}
.contain-li span:first-child{
  color:#222222;
}
