/**index.wxss**/
@import "/component/tipToast/tipToast.wxss"; 
page {
  background-color: #f6f9fb;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
  color: #939393;
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
  padding: 0;

}

.mealorder .banner {
  width: 751rpx;
  height: 314rpx;
}

.mealorder .imagebox image {
  width: 22rpx;
  height: 22rpx;
  margin-top: -10rpx;
}

.mealorder .picleft {
  float: left;
  margin-left: 83rpx;
}

.mealorder .picright {
  float: right;
  margin-right: 83rpx;
}

.mealorder .choicebox {
  width: 708rpx;
  height: 495rpx;
  padding: 0 42rpx;
  box-sizing: border-box;
  background-color: #fff;
  color: #595959;
  margin: 0 auto;
  border: 1.5rpx solid #e5e5e5;
}

.mealorder .choicebox>view {
  height: 112rpx;
  line-height: 112rpx;
  border-bottom: 1rpx solid #c7c7c7;
  width: 100%;
  display: inline-block;
  font-size:28rpx;
}

.mealorder .choicebox image {
  width: 17rpx;
  height: 27rpx;
  float: right;
  margin-top: 45rpx;
}


.mealorder button {
  background-color: #1bb150;
  color: #fff;
  margin-top: 28rpx;
  margin-bottom: 28rpx;
  font-size:30rpx;
}

/*日历*/

.usermotto {
    margin-top: 100px;
}

.other-button-hover {
    background-color: pink;
}